{
    "manifest_version": 2,
    "name": "CSS Debug",
    "version": "0.9",

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "web_accessible_resources": [
    "debug.css"
  ]
}
